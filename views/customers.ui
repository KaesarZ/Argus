/*	QComboBox	*/
QComboBox{
	height: 32px;
	font-family: Roboto;
	font-size: 16px;
	color: #2c3e50;
	border-radius: 4px;
	border: 1px solid #bdc3c7;
}
QComboBox QAbstractItemView {
	selection-background-color: #2c3e50;
	selection-color: #ffffff;
}
QComboBox::drop-down {
	border: 0;
	border-radius: 4px;
}
QComboBox::down-arrow{   
	image: url(resources/icons/arrows-down.png); 
	width: 14px;
	margin-right: 5px;
}
/*	QDateEdit	*/
QDateEdit{
	height: 32px;
	font-family: Roboto;
	font-size: 16px;
	color: #2c3e50;
