	selection-background-color: #2c3e50;
	selection-color: #ffffff;
}
QDateEdit::drop-down{ border: 0; }
QDateEdit::down-arrow{
	image: url(resources/icons/arrows-down.png); 
	width: 14px;
}
/*	QCalendarWidget	*/
QCalendarWidget {
	font-family: Roboto;
	font-size: 12px;
}
QCalendarWidget QAbstractItemView {
	background-color:#ffffff; 
	selection-background-color: #2c3e50;
	selection-color: #ecf0f1;
}
